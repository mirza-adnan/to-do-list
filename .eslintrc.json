{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-alert": 0,
        "quotes": [2, "double", { "avoidEscape": true }]
    }
}
